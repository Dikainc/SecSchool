@model EnterpriseSchool.Web.Areas.Admin.ViewModels.UserViewModel
@{
    ViewBag.Title = "UploadedStaff";
}

<div class="col-md-12">

    <table class="table table-bordered">
        <thead>
            <tr>
                <th>S/N</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Other Name</th>
                <th>User Name</th>
                <th>Password</th>
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < Model.UploadStaff.Count(); i++)
            {
                int b = i + 1;
                <tr>
                    <td>@b</td>
                    <td>@Model.UploadStaff[i].FirstName</td>
                    <td>@Model.UploadStaff[i].LastName</td>
                    <td>@Model.UploadStaff[i].OtherName</td>
                    <td>@Model.UploadStaff[i].Username</td>
                    <td>@Model.UploadStaff[i].Password</td>
                </tr>
            }
        </tbody>
    </table>

    @Html.ActionLink("Add Staff", "SaveStaff", null, new { @class = "btn btn-primary" })

</div>
